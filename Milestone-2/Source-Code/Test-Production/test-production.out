--VIEW FOODS EXPIRING SOON
SELECT name, quantity, expiration_date
FROM Items
WHERE Items.id = 11 AND (CURRENT_DATE >= expiration_date - INTEGER '4')
ORDER BY expiration_date ASC, name ASC;
